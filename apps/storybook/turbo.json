{
  "extends": ["//"],
  "tasks": {
    "dev": {
      "dependsOn": ["^build"],
      "with": ["frey-ui#dev"],
      "cache": false,
      "persistent": true
    },
    "storybook": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": ["storybook-static/**"]
    },
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck", "^build"],
      "outputs": [
        "node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "node_modules/.tmp/tsconfig.node.tsbuildinfo"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**", "html/**"]
    }
  }
}
