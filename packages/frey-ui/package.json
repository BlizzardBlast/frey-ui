{
  "name": "frey-ui",
  "version": "1.0.10",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "license": "MIT",
  "sideEffects": [
    "**/*.css",
    "**/*.module.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/types/src/*/index.d.ts",
      "import": "./dist/esm/*/index.js",
      "require": "./dist/cjs/*/index.js"
    },
    "./style.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlizzardBlast/frey-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rimraf dist tsconfig.lib.tsbuildinfo && rollup -c",
    "typecheck": "tsc -b --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "test": "vitest run",
    "test:coverage:blob": "vitest run --coverage.enabled=true --reporter=blob --outputFile.blob=./.vitest-reports/blob.json --reporter=junit --outputFile.junit=./.junit-reports/junit.xml"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "jest-axe": "^10.0.0",
    "jsdom": "^28.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rimraf": "^6.1.2",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
